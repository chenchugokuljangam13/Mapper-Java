{
  "id": "nutritionalMetabolic",
  "cards": [{
    "id": "decline",
    "type": "declineCard",
    "access": "write"
  },
    {
      "id": "weight",
      "type": "deltaListCard",
      "required": true,
      "declinable": true,
      "deferrable": true,
      "access": "write",
      "data": {
        "listHeaders": ["date", "weightLbs", "weightKg", "bmi"],
        "deltaFields": ["date", "weightLbs", "weightKg", "bmi"]
      },
      "inputs": {
        "weight": {
          "type": "conversion",
          "rules": {
            "required": true,
            "aUnit": "lbs",
            "bUnit": "kg",
            "conversionFactorAB": 0.45359237,
            "precision": "1.0-2",
            "max": 999.99,
            "min": 0
          }
        }
      }
    },
    {
      "id": "weightChange",
      "type": "formCard",
      "access": "write",
      "declinable": true,
      "inputs": {
        "normalWeight": {
          "type": "number",
          "rules": {
            "min": 0
          }
        },
        "priorWeight": {
          "type": "number",
          "rules": {
            "min": 0
          }
        },
        "priorWeightLoss": {
          "type": "radio",
          "options": [
            "yes",
            "no",
            "unknown"
          ]
        },
        "priorWeightLossValue": {
          "type": "number"
        },
        "priorWeightLossPeriod": {
          "type": "radio",
          "options": [
            "weightLossFivePercentOneMonth",
            "weightLossTenPercentSixMonth"
          ]
        },
        "priorWeightLossUnknownReason": {
          "type": "radio",
          "options": [
            "difficultySwallowing",
            "nutritionalStatus"
          ],
          "supportOther": true
        }
      },
      "dependencies": [{
        "id": "priorWeightLoss",
        "values": ["yes"],
        "type": "enable",
        "fields": [
          "priorWeightLossValue",
          "priorWeightLossPeriod"
        ]
      },
        {
          "id": "priorWeightLoss",
          "values": ["unknown"],
          "type": "enable",
          "fields": [
            "priorWeightLossUnknownReason"
          ]
        }
      ],
      "layout": [{
        "rows": [{
          "id": "normalWeight"
        },
          {
            "id": "priorWeightLoss"
          },
          {
            "id": "priorWeightLossUnknownReason"
          }
        ]
      },
        {
          "rows": [{
            "id": "priorWeight"
          },
            {
              "id": "priorWeightLossValue"
            },
            {
              "id": "priorWeightLossPeriod"
            }
          ]
        }
      ]
    },

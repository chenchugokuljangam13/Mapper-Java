    {
      "id": "needs",
      "type": "historyListCard",
      "declinable": true,
      "data": {
        "listHeaders": ["category", "addedBy", "date", "description"]
      },
      "inputs": {
        "category": {
          "componentId": "category",
          "type": "select",
          "options": [
            "anticipatoryGrief",
            "communityResources",
            "financial",
            "funeral",
            "interpersonalSupport",
            "legal",
            "placement",
            "volunteer"
          ],
          "rules": {
            "required": true
          }
        },
        "description": {
          "componentId": "description",
          "type": "textarea",
          "rules": {
            "rows": 3,
            "maxLength": 150,
            "required": true
          }
        }
      }
    },
    {
      "id": "risks",
      "type": "historyListCard",
      "declinable": true,
      "data": {
        "listHeaders": ["category", "person", "score", "addedBy", "date", "description"]
      },
      "inputs": {
        "category": {
          "componentId": "category",
          "type": "select",
          "options": [
            "safety",
            "suicide"
          ],
          "rules": {
            "required": true
          }
        },
        "person": {
          "componentId": "person",
          "type": "select",
          "supportOther": true,
          "options": [
            "patient",
            "pcg",
            "family"
          ],
          "rules": {
            "required": true
          }
        },
        "score": {
          "componentId": "score",
          "type": "range",
          "rules": {
            "min": 1,
            "max": 10,
            "step": 1,
            "snaps": true,
            "required": true
          }
        },
        "description": {
          "componentId": "description",
          "type": "textarea",
          "rules": {
            "rows": 3,
            "maxLength": 150,
            "required": true
          }
        }
      },
      "dependencies": [{
        "id": "category",
        "values": ["suicide"],
        "type": "enable",
        "fields": ["person", "score"]
      }]
    },
